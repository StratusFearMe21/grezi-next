{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "grz",
      "camelcase": "Grz",
      "title": "Grz",
      "scope": "source.grz",
      "file-types": [
        "grz"
      ],
      "injection-regex": "^grz$",
      "highlights": "queries/highlights.scm",
      "class-name": "TreeSitterGrz"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Parser for Grezi grz files",
    "authors": [
      {
        "name": "Isaac Mills"
      }
    ],
    "links": {
      "repository": "https://github.com/StratusFearMe21/tree-sitter-grz"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": true
  }
}
